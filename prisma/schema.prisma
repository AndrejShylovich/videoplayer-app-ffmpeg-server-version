

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Status {
  trial
  sub
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  status    Status
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}